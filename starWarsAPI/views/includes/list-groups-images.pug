ol.bg-light.list-group.list-group-horizontal-md(id='order-list') 
    each val, index in data
        li(id='list-items')
            .card(style='width: 18rem;')
                a(data-bs-toggle='modal' data-bs-target='#exampleModal')
                    img.card-img-top(src='https://img.pokemondb.net/sprites/home/normal/charmander.png' alt='...')
                .card-body
                    p.card-text
                    | #{val}.
