ol.list-group.list-group-horizontal-md(id='order-list-container') 
    each val, index in data
        li(id='list-items')
            .card.border-warning.bg-secondary.text-white(id='card' style='width: 18rem;')
                if title === 'Characters'
                    a(data-bs-toggle='modal' data-bs-target='#dataModal'+index)
                        img.card-img-top.border-bottom.border-warning(src=val[1] alt='Star Wars Character')
                    .card-body.text-center
                        p.card-text
                            | #{val[0]}
                else if title === 'Memes'
                    a(data-bs-toggle='modal' data-bs-target='#memeModal'+index)
                        img.card-img-top(src=val[2] alt='Star Wars Meme')
                    .card-body.text-center
                        p.card-text
                            | #{val[1]}
                else 
                    a(data-bs-toggle='modal' data-bs-target='#dataModal'+index)
                        .card-body.text-center
                            p.card-text
                                | #{val}
        if title !== 'Memes'
            include modal.pug
        else 
            include memeModal.pug
